    "import re\n",
    "\n",
    "def money_data_finder(text):\n",
    "    pattern = r'(\\$[\\d.]+)|([\\d.]+\\$)|([\\d]+/[\\d]+/[\\d]+)|([\\d]+-[\\d]+-[\\d]+)'\n",
    "    match = re.findall(pattern, text)\n",
    "    if(match):\n",
    "        return match"
   ]
