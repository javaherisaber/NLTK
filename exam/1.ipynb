   "source": [
    "from nltk.tokenize import word_tokenize\n",
    "\n",
    "def word_joiner(sent):\n",
    "    words = word_tokenize(sent)  # extract tokens\n",
    "    return [s[:2] + s[-2:] for s in words if len(s) >= 4]"
   ]
  },
